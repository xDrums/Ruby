Rails.application.routes.draw do
  root 'welcome#index'
  get "special", to: "welcome#index" #renombro la ruta como special y me dirige a welcome index
  
  resources: articles
    get "/articles"
    post "/articles"
    delete "/articles"
    get "/articles/:id"
    get "/articles/:new"
    get "/articles/:id/edit"
    patch "/articles/:id"
    put "/articles/;id"
    
end
